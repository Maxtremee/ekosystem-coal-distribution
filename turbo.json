{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "db-generate": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db-migrate-dev": {
      "dependsOn": ["db-generate"],
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db-migrate": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "^db-generate"],
      "outputs": [".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "type-check": {
      "dependsOn": ["^db-generate"],
      "cache": false
    }
  },
  "globalEnv": [
    "DATABASE_URL",
    "NEXTAUTH_SECRET",
    "NEXTAUTH_URL",
    "AUTH0_CLIENT_ID",
    "AUTH_CLIENT_SECRET",
    "AUTH0_AUDIENCE",
    "AUTH0_ISSUER_BASE_URL",
    "AUTH0_BASE_URL"
  ]
}
